{
  "name": "dex-turbo",
  "private": "true",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/EthereumCommonwealth/Dex223-UI.git",
  "license": "MIT",
  "scripts": {
    "build": "turbo build",
    "build:web": "cd apps/web && npm run build",
